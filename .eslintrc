{
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "no-console": 1,
    "react/no-did-mount-set-state": 1,
    "react/no-direct-mutation-state": 1
  },
  "globals": {
    "jQuery": true,
    "$": true
  }
}
